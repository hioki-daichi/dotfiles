openOutdatedComment -> {{
  // Array.from(document.getElementsByClassName('outdated-comment')).forEach(l => l.classList.add('open'));
  Array.from(document.getElementsByClassName('show-outdated-button')).forEach(a => a.click());
}}
site 'https://github.com/*/*' {
  map O :call openOutdatedComment<CR>
}
yankLinkMarkdown -> {{
  var title = window.document.title.split(" by ", 2)[0].replace(/(<|>)/g, '\\$1')
  var LinkMarkdown = `[${title}](${window.location.href})`
  Clipboard.copy(LinkMarkdown);
  Status.setMessage(LinkMarkdown, 2);
}}
openServicesMenu -> {{
  document.getElementById("nav-servicesMenu").click();
}}
site 'https://*.?console.aws.amazon.com/*/*' {
  map t :call openServicesMenu<CR>
}
